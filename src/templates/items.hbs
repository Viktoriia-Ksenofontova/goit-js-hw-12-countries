{{#each this}}
<li class="info">
  <article class="card">
    <img src="{{flag}}" alt="flag of {{name}}" class="card__image" />
    <div class="card__content">
      <h2 class="country__name">{{name}}</h2>
      <p class="capital">
        Capital: {{capital}}
      </p>

      <p class="population">
        Population: {{population}}
      </p>


      <p class="languages">
        languages:
        {{#each languages}}
        {{this.name}}
        {{/each}}
      </p>
    </div>

  </article>
</li>
{{/each}}